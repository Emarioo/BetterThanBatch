@TEST-CASE inline_asm_stack
a = cast_unsafe<i32> asm {
    mov eax, 23
    push rax
}
test 23; a;

struct Pack {
    x: i32;
    y: char;
}
b = cast_unsafe<Pack> asm {
    mov eax, 'A'
    push rax
    mov eax, 5
    push rax
}

test 5; b.x;
test 'A'; b.y;

@TEST-CASE inline_asm_labels

test 15; cast_unsafe<i32> asm {
    mov ecx, 0
    mov eax, 0
loop:
    cmp ecx, 5
    je loop_end
    add ecx, 1
    add eax, ecx
    jmp loop
loop_end:
    push rax  // return value
}