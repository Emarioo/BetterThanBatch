{
  "name": "btb-lang",
  "displayName": "BTB Lang",
  "author": "Emarioo",
  "publisher": "Emarioo",
  "description": "Syntax highlight for btb language",
  "icon": "img/icon.png",
  "version": "0.1.0",
  "engines": {
    "vscode": "^1.77.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "betterthanbatch",
      "aliases": ["Better Than Batch", "betterthanbatch"],
      "extensions": [".btb"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "betterthanbatch",
      "scopeName": "source.btb",
      "path": "./syntaxes/betterthanbatch.tmLanguage.json"
    }]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Emarioo/BetterThanBatch"
  },
  "homepage": "https://github.com/Emarioo/BetterThanBatch>/tree/main/btb-lang"
}
