
#import "Math"
#import "Logger"

gen := create_random_generator();

iterations: u64 = 10000;
bits32: u32[32];
bits64: u32[64];
for 0..iterations {
    ran32 := gen.rand32();   
    ran64 := gen.rand64();

    for 0..32 {
        bits32.ptr[nr] += ran32 & 1u;
        ran32 = ran32 >> 1;
    }
    for 0..64 {
        bits64.ptr[nr] += ran64 & cast<u64>1u;
        ran64 = ran64 >> 1;
    }
}
log("rand32: bit frequencies:")
for 0..32 {
    log("Bit",nr,bits32.ptr[nr])
}
log("rand64: bit frequencies:")
for 0..64 {
    log("Bit",nr,bits64.ptr[nr])
}